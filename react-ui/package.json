    "formik": "^2.4.5",
    "framer-motion": "^11.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "redux-persist": "^6.0.0",
    "yup": "^1.3.3"
